      operation 360834 "sendNotification"
	public explicit_return_type "void"
	nparams 2
	  param in name "PageElement" explicit_type ""
	  param in name "Notification" explicit_type "\\content\\pageelement\\Notification"
	
	
	preserve_php_body_indent php_def "  ${comment}${final}${visibility}${abstract}${static}function ${name}${(}${p0}${v0}, ${p1}${v1}${)}${type}
