!!!235906.php!!!	display() : string

			global $Visitor;
			$this->addValueElement('metas', array(
				'charset' => 'utf-8',
				'lang'    => $Visitor->getConfiguration('lang'),
			));
			if ($this->getElement('title'))
			{
				$this->addValueElement('title', $GLOBALS['lang']['html_title_prefix']);
			}
			return parent::display();
